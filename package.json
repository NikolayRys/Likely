{
  "name": "ilyabirman-likely",
  "description": "The social sharing buttons that arenâ€™t shabby",
  "version": "2.7.0-dev",
  "repository": {
    "type": "git",
    "url": "https://github.com/ilyabirman/Likely.git"
  },
  "author": {
    "name": "Ilya Birman",
    "url": "http://ilyabirman.net/"
  },
  "contributors": [
    {
      "name": "Nikolay Rys",
      "email": "nikolay@rys.me",
      "url": "https://www.linkedin.com/in/nikolay-rys/"
    },
    "Aleksandra Rys",
    "Viktor Karpov",
    "Ivan Akulov"
  ],
  "license": "MIT",
  "main": "release/likely-commonjs.js",
  "scripts": {
    "build:development": "webpack --progress --profile --node-env=development --env commonjs=false && webpack --progress --profile --node-env=development --env commonjs=true",
    "build:production": "webpack --progress --profile --node-env=production --env commonjs=false && webpack --progress --profile --node-env=production --env commonjs=true",
    "build": "npm run build:development && npm run build:production",
    "check-codestyle": "eslint ./",
    "fix-codestyle": "eslint ./ --fix",
    "integration-tests": "mocha --exit test/index.js",
    "test": "npm run integration-tests && npm run check-codestyle",
    "prepare-release": "mkdir release/archive && cp release/likely.min.js release/archive/likely.js && cp release/likely.min.css release/archive/likely.css",
    "zip": "bestzip release/$npm_package_name-$npm_package_version.zip release/archive/*",
    "clean-up-release": "rm -rf release/archive",
    "release": "npm run prepare-release && npm run zip && npm run clean-up-release",
    "prepublish": "in-publish && npm run test || not-in-publish"
  },
  "files": [
    "release/likely.js",
    "release/*.css",
    "release/*.js",
    "release/*.map",
    "LICENCE.txt"
  ],
  "devDependencies": {
    "@babel/core": "^7.17.10",
    "@babel/preset-env": "^7.17.10",
    "babel-loader": "^8.2.5",
    "babel-plugin-add-module-exports": "^1.0.4",
    "bestzip": "^2.2.1",
    "chai": "^4.3.6",
    "chai-as-promised": "^7.1.1",
    "chromedriver": "^100.0.0",
    "css-loader": "^6.7.1",
    "css-minimizer-webpack-plugin": "^3.4.1",
    "cssnano": "^5.1.7",
    "eslint": "^8.14.0",
    "in-publish": "^2.0.1",
    "mini-css-extract-plugin": "^2.6.0",
    "mocha": "^9.1.4",
    "selenium-webdriver": "^4.1.2",
    "static-server": "^2.2.1",
    "style-loader": "^3.3.1",
    "stylus": "^0.57.0",
    "stylus-loader": "^6.2.0",
    "terser-webpack-plugin": "^5.3.1",
    "webpack": "^5.72.0",
    "webpack-cli": "^4.9.2"
  }
}
